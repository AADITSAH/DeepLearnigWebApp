<% layout ('layouts/boilerplate')%>
<h1 class="text-center">EmoteDetection</h1>

<div class="text-center">
    <a href="/emodetect/new" >Predict New Emote</a>
</div>
<% for (let ed of emote_detection){%>
<div class="card sm-6">
    <div class="row">
        <div class="col-md-2">
            <img class="img-fluid mh-2 mw-2" alt="" src="data:ed/<%=ed.img.contentType%>;base64,
            <%=ed.img.data.toString('base64')%>">
        </div>
        <div class="col-md-8">
            <div class="card-body">
                <h5 class="card-title">Emotion-Type : <%= ed.emotion_type %> </h5>
                <p class="card-text">Probability : <%= ed.probability %></p>    
            </div>
        </div>
    </div>
</div>
<% }%>